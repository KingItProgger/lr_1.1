def say(a):
    print(a)
say('hello')

names=['ivan','petr','roman','dmitri']
for name in names:
    say('Hello, ')
    print(name)

class Person():
    def __init__ (self,name,age,height,weight):
        self.name=name
        self.age=age
        self.height=height
        self.weight=weight
    
 def __str__ (self):
        return str(self.name,self.age,self.height,self.weight)

print ('hello,world')    

def plus(a,b):
    print (a+b)
