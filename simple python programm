def say(a):
    print(a)
say('hello')

names=['ivan','petr','roman','dmitri']
for name in names:
    say('Hello, ')
    print(name)

class Person():
    def __init__ (self,name,age,height,weight):
        self.name=name
        self.age=age
        self.height=height
        self.weight=weight
    
